{
    "name": "maartenpaauw/filament-cashier-stripe-billing-provider",
    "description": "Filament billing provider for Laravel Cashier",
    "keywords": [
        "maartenpaauw",
        "filament-cashier-stripe-billing-provider"
    ],
    "homepage": "https://github.com/maartenpaauw/filament-cashier-stripe-billing-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten Paauw",
            "email": "maartenpaauw@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Maartenpaauw\\Filament\\Cashier\\FilamentCashierStripeBillingProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maartenpaauw\\Filament\\Cashier\\FilamentCashierStripeBillingProvider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
